<application>
  <component name="FileTypeManager" version="16">
    <ignoreFiles list="*.hprof;*.pyc;*.pyo;*.rbc;*~;.DS_Store;.git;.hg;.svn;CVS;RCS;SCCS;__pycache__;_svn;rcs;" />
    <extensionMap>
      <mapping ext="dust" type="Dust" />
      <mapping ext="vash" type="HTML" />
      <mapping pattern=".bowerrc" type="JSON" />
      <mapping pattern="css" type="PLAIN_TEXT" />
      <mapping pattern=".gitkeep" type="PLAIN_TEXT" />
      <mapping pattern="Procfile" type="PLAIN_TEXT" />
      <mapping pattern=".editorconfig" type="PLAIN_TEXT" />
      <mapping ext="csv" type="PLAIN_TEXT" />
      <mapping ext="pde" type="PLAIN_TEXT" />
      <mapping ext="svg" type="XML" />
    </extensionMap>
  </component>
</application>